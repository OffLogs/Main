<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping 
        xmlns="urn:nhibernate-mapping-2.2"
        namespace="OffLogs.Business.Db.Entity" 
        assembly="OffLogs.Business">

    <class name="RequestLogEntity" table="request_logs">
        <id name="Id">
            <column name="id" sql-type="int" not-null="true"/>
            <generator class="native" />
        </id>
        <property name="Type" type="OffLogs.Business.Db.Types.RequestLogTypeConstantType,OffLogs.Business">
            <column name="type" length="4" not-null="true" />
        </property>
        <property name="ClientIp">
            <column name="client_ip" length="50" not-null="true" />
        </property>
        <property name="Data">
            <column name="data" not-null="true" />
        </property>
        <property name="CreateTime">
            <column name="create_time" sql-type="datetime" not-null="true" />
        </property>
    </class>
</hibernate-mapping>